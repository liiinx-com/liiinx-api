{
  "testEnvironment": "node",
  "preset": "ts-jest",
  "rootDir": "./",
  "modulePaths": ["<rootDir>"],
  "testRegex": ".e2e-spec.ts$",
  "moduleNameMapper": {
    "^src$": "<rootDir>/../src",
    "^src/(.+)$": "<rootDir>/../src/$1"
  },
  "modulePathIgnorePatterns": ["src/typings"],
  "testPathIgnorePatterns": [
    "/node_modules./",
    "<rootDir>/(coverage|dist|lib|tmp)./"
  ]
}
